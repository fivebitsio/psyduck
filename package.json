{
  "name": "psyduck",
  "private": true,
  "type": "module",
  "workspaces": [
    ".",
    "track",
    "web",
    "docs"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:standalone": "node src/index.ts",
    "dev:web": "pnpm --filter=web dev",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run dev:web\"",
    "migrate": "tsx bin/migrate.ts",
    "db": "duckdb data/psyduck.db",
    "prepare-config": "tsx bin/config.ts",
    "track:build": "pnpm --filter=@psyduck/track build",
    "track:package": "pnpm --filter=@psyduck/track package",
    "track:clean": "pnpm --filter=@psyduck/track clean",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev:docs": "pnpm --filter=docs dev"
  },
  "dependencies": {
    "@duckdb/node-api": "1.3.2-alpha.25",
    "@inquirer/prompts": "^7.8.6",
    "async-mutex": "^0.5.0",
    "countries-and-timezones": "^3.8.0",
    "dotenv": "^17.2.3",
    "hono": "^4.9.1",
    "lowdb": "^7.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@eslint-react/eslint-plugin": "^1.53.0",
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.39.1"
  }
}
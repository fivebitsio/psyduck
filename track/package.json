{
  "name": "@psyduck/track",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "main": "track.js",
  "scripts": {
    "package": "bun run prebuild && esbuild track.js --outfile=dist/track.js --bundle --minify",
    "clean": "rimraf dist",
    "prebuild": "bun run clean",
    "build": "bun run package",
    "dev": "nodemon --watch track.js --exec 'node track.js'"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "nodemon": "^3.1.0",
    "rimraf": "^6.0.1"
  }
}
